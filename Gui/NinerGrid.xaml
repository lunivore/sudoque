<UserControl x:Class="Sudoque.Gui.NinerGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Gui="clr-namespace:Sudoque.Gui">
    <Border BorderThickness="3" BorderBrush="DarkGray">
        <Grid ShowGridLines="False">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Gui:CellDocument Grid.Row="0" Grid.Column ="0"/>
            <Gui:CellDocument Grid.Row="1" Grid.Column ="0"/>
            <Gui:CellDocument Grid.Row="2" Grid.Column ="0"/>
            <Gui:CellDocument Grid.Row="0" Grid.Column ="1"/>
            <Gui:CellDocument Grid.Row="1" Grid.Column ="1"/>
            <Gui:CellDocument Grid.Row="2" Grid.Column ="1"/>
            <Gui:CellDocument Grid.Row="0" Grid.Column ="2"/>
            <Gui:CellDocument Grid.Row="1" Grid.Column ="2"/>
            <Gui:CellDocument Grid.Row="2" Grid.Column ="2"/>
        </Grid>
    </Border>
</UserControl>
